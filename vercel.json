{
  "version": 2,
  "builds": [
    {
      "src": "build_static.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/static/index.html"
    }
  ],
  "functions": {
    "build_static.py": {
      "runtime": "python3.9"
    }
  }
}